import "./address.aqua"
import "../src/aqua/Web3.aqua"

func demoGetBalance() -> string:
    on PEER via RELAY:
        res <- Eth.getBalance("0x00000000219ab540356cbb839cbe05303d7705fa", nil)
    <- res

func demoGetAccounts() -> []string:
    on PEER via RELAY:
        res <- Eth.getAccounts()
    <- res

func demoGetBlockNumber() -> u64:
    on PEER via RELAY:
        res <- Eth.getBlockNumber()
    <- res

func demoGetFeeHistory() -> FeeHistory:
    on PEER via RELAY:
        percentiles <- Convert.strTou16("[25, 50, 100]")
        res <- Eth.getFeeHistory(2, 1000, percentiles)
    <- res

func demoGetGasPrice() -> string:
    on PEER via RELAY:
        res <- Eth.getGasPrice()
    <- res
